    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-lazy-load-image-component": "^1.6.0",
    "react-router-dom": "^6.14.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^4.32.5",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/react-lazy-load-image-component": "^1.6.0",
